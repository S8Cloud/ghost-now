{
    "name": "ghost-zeit",
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@now/node-server",
            "config": {
                "maxLambdaSize": "50mb"
            }
        }
    ],
    "env": {
        "url": "https://ghost-zeit.now.sh",
        "NODE_ENV": "production"
    },
    "alias": [
        "ghost-zeit.now.sh"
    ]
}
